<div class="container">
  <div class="row">
    <%= form_with model: @user, local: true do |f| %>
      <div class="form-group">
        <%= f.label :email %></br>
        <%= f.email_field :email %>
      </div>
      <div class="form-group">
        <%= f.label :password %></br>
        <%= f.password_field :password %>
      </div></br>
      <div class="form-group">
        <%= f.submit 'ログイン' %>
      </div>
    <% end %>
  </div>  
</div>
